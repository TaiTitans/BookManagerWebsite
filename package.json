{
  "name": "customer_fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec \"npm run vue:dev && npm run tailwind:dev\"",
    "serve": "npm run dev",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "vue:dev": "vue-cli-service serve",
    "tailwind:dev": "npx tailwindcss -i /src/assets/main.css -o /src/assets/base.css --watch"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "nodemon": "^3.1.0",
    "primeicons": "^6.0.1",
    "primevue": "^3.50.0",
    "vue": "^3.2.13",
    "vue-router": "^4.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
